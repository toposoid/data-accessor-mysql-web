# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

# An example controller showing a sample home page
POST     /addDocumentAnalysisResultHistory                              controllers.DocumentAnalysisResultHistoryController.add()
POST     /searchDocumentAnalysisResultHistoryByDocumentIdAndStateId     controllers.DocumentAnalysisResultHistoryController.searchByDocumentIdAndStateId()
POST     /getKnowledgeRegisterHistoryTotalCountByDocumentId             controllers.DocumentAnalysisResultHistoryController.getTotalCountByDocumentId()
POST     /getAllDocumentAnalysisResultStates                            controllers.DocumentAnalysisResultStatesController.getAll()
POST     /addKnowledgeRegisterHistory                                   controllers.KnowledgeRegisterHistoryController.add()
POST     /searchKnowledgeRegisterHistoryByDocumentId                    controllers.KnowledgeRegisterHistoryController.searchByDocumentId()
POST     /searchKnowledgeRegisterHistoryByPropositionId                 controllers.KnowledgeRegisterHistoryController.searchByPropositionId()
POST     /getKnowledgeRegisterHistoryCountByDocumentId                  controllers.KnowledgeRegisterHistoryController.getCountByDocumentId()
POST     /getAllKnowledgeRegisterStates                                 controllers.KnowledgeRegisterStatesController.getAll()
POST     /addNonSentenceSections                                        controllers.NonSentenceSectionsController.add()
POST     /searchNonSentenceSectionsByDocumentId                         controllers.NonSentenceSectionsController.searchByDocumentId()
POST     /getAllNonSentenceTypes                                        controllers.NonSentenceTypesController.getAll()
